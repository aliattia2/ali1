   #include <cstdio>
  using namespace std;
 
  int main()
 
  {
     int x =100 ; 
int *p =& x;
  printf("x = %d\n" ,x);
    printf("p = %d\n" ,p);
  printf("p = %d\n" ,*p);
  }
 
