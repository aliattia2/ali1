  fl =200;
  ch = 8;
  sh = 4;

  printf ("in = %d\n" ,in);
   printf ("in = %d\n" ,sizeof in);
    printf ("fl = %d\n" ,sizeof (fl));
      printf ("sh = %d\n" ,sizeof sh);
        printf ("li = %d\n" ,li);
          printf ("ch = %d\n" ,ch);
      
           
